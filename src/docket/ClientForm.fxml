<AnchorPane maxHeight="425.0" maxWidth="800.0" minHeight="425.0" minWidth="800.0" prefHeight="425.0" prefWidth="800.0" styleClass="border-pane" stylesheets="@resources/stylesheet.css" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
